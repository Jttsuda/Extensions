{
  "manifest_version": 2,
  "name": "Center YouTube Search",
  "version": "1.1",

  "description": "Adds a left margin to Youtube search results.",

  "icons": {
    "48": "icon/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["center.js"]
    }
  ],

  "browser_specific_settings": {
  "gecko": {
    "id": "extension@Jttsuda.github.com"
    }
  }

}